@model PackManagementSystem.DTOs.StaffDto.StaffDto
<div>
    <h4 style="alignment: center; color: maroon; padding-bottom: 20px; padding-left: 20px"> Delete the selected Staff? </h4>
    <div>
        <table class="table">
            <tbody>
            <tr>
                <th>
                    <form asp-action="DeleteStaff">
                        <input type="hidden" asp-for="Id"/>
                        <input type="submit" value="Confirm" class="btn btn-danger">
                    </form>
                </th>
                <th><a asp-action="AdminMenu" class="btn badge-success">Decline</a></th>
            </tr>
            </tbody>
        </table>
    </div>
</div>